
<%= form_for [@photo, @comment] do |f| %>
<%= f.hidden_field :photo_id %>
     <%= f.label :content %>
     <%= f.text_field :content, required:true %>
     
     <br>       
     <%= f.submit "Add the comment " %> 
<% end %>

<script>
function goBack() {
  window.history.back();
}
</script>